<Project>
  <ImportGroup>
    @import_props@
  </ImportGroup>

  <PropertyGroup Label="Globals">
    <WindowsTargetPlatformVersion>10.0</WindowsTargetPlatformVersion>
    <WindowsTargetPlatformMinVersion>10.0.22000.0</WindowsTargetPlatformMinVersion>
    <CppWinRTProjectLanguage>C++/WinRT</CppWinRTProjectLanguage>
    <CppWinRTLibs>false</CppWinRTLibs>
    <CppWinRTOptimized>true</CppWinRTOptimized>
    <XamlLanguage Condition="'$(XamlLanguage)' == ''">CppWinRT</XamlLanguage>
    <CppWinRTUsePrefixes>false</CppWinRTUsePrefixes>
    <GeneratedFilesDir>$([MSBuild]::NormalizeDirectory('@GeneratedFilesDir@', $(ProjectName)))</GeneratedFilesDir>
    <WindowsPackageType>none</WindowsPackageType>
    <AppxPackage>false</AppxPackage>
    <CharacterSet>Unicode</CharacterSet>
    <ApplicationType>Windows Store</ApplicationType>
    <WindowsAppSdkBootstrapInitialize>false</WindowsAppSdkBootstrapInitialize>
    <AppContainerApplication>false</AppContainerApplication>
    <UseWinUI>true</UseWinUI>
  </PropertyGroup>
</Project>
